@article{stopaConstructingSelfRole2010,
  title = {Constructing a Self: {{The}} Role of Self-Structure and Self-Certainty in Social Anxiety},
  shorttitle = {Constructing a Self},
  author = {Stopa, Lusia and Brown, Mike A. and Luke, Michelle A. and Hirsch, Colette R.},
