@article{petersonTickingSocialClock1996,
  title = {The {{Ticking}} of the {{Social Clock}}: {{Adults}}' {{Beliefs}} about the {{Timing}} of {{Transition Events}}},
  shorttitle = {The {{Ticking}} of the {{Social Clock}}},
  author = {Peterson, Candida C.},
  year = {1996},
